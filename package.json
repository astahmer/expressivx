{
    "name": "expressivx",
    "version": "0.0.0",
    "description": "Describe your back-end API with JSX",
    "main": "index.js",
    "scripts": {
        "tsc": "tsc --watch -p tsconfig.json",
        "dev": "yarn tsc & yarn watch",
        "start": "node --inspect=127.0.0.1:9229 ./build/main.js",
        "watch": "nodemon"
    },
    "author": "Alexandre Stahmer",
    "license": "ISC",
    "dependencies": {
        "express": "^4.17.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-reconciler": "^0.26.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.11",
        "@types/node": "^14.14.41",
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.2",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "prettier-plugin-sorted": "^2.0.0",
        "typescript": "^4.1.2"
    },
    "importSort": {
        ".js, jsx, .ts, .tsx": {
            "options": {
                "ignoreTsConfig": true
            }
        }
    }
}
